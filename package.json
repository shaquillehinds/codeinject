{
  "name": "tscodeinject",
  "version": "1.1.4",
  "description": "Inject code into your typescript files",
  "main": "dist/index.js",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/shaquillehinds/codeinject"
  },
  "author": {
    "name": "Shaquille Hinds",
    "email": "shaquillehinds72@hotmail.com",
    "url": "https://github.com/shaquillehinds"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "ISC",
  "private": false,
  "type": "module",
  "bin": "./dist/index.bin.js",
  "scripts": {
    "deploy": "bash scripts/deploy.sh",
    "build": "rm -rf dist && tsc -p ./ && tsc-alias -p ./tsconfig.json && chmod +x dist/index.bin.js",
    "pre:start": "yarn build",
    "start": "node dist/",
    "start:dev": "yarn build && concurrently \"tsc -w\" \"tsc-alias -w\"",
    "npm-pub": "yarn build && npm publish --access public"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "chalk-animation": "^2.0.3",
    "commander": "^11.0.0",
    "events": "^3.3.0",
    "figlet": "^1.6.0",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.2.8",
    "jscodeshift": "^0.15.0",
    "nanospinner": "^1.1.0",
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@types/chalk-animation": "^1.6.1",
    "@types/events": "^3.0.0",
    "@types/figlet": "^1.5.6",
    "@types/gradient-string": "^1.1.2",
    "@types/inquirer": "^9.0.3",
    "@types/jscodeshift": "^0.11.6",
    "@types/node": "^20.4.2",
    "concurrently": "^8.2.0",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  }
}
