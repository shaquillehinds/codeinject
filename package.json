{
  "name": "tscodeinject",
  "version": "2.3.3",
  "description": "Inject code into your typescript files",
  "release": {
    "repositoryUrl": "https://github.com/shaquillehinds/codeinject",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/shaquillehinds/codeinject"
  },
  "author": {
    "name": "Shaquille Hinds",
    "email": "shaqdulove@gmail.com",
    "url": "https://github.com/shaquillehinds"
  },
  "files": [
    "dist/**/*"
  ],
  "license": "ISC",
  "private": false,
  "type": "module",
  "bin": "./dist/esm/index.bin.js",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "scripts": {
    "deploy": "bash scripts/deploy.sh",
    "build:cjs": "tsc -p ./tsconfig.common.json && node scripts/js-to-cjs.cjs && tsc-alias -p ./tsconfig.common.json",
    "build:esm": "tsc -p ./ && tsc-alias -p ./tsconfig.json && node scripts/js-to-mjs.cjs",
    "build": "rm -rf dist && yarn build:cjs && yarn build:esm && chmod +x dist/cjs/index.bin.js && chmod +x dist/esm/index.bin.js",
    "prestart": "yarn build",
    "start": "node dist/",
    "start:dev": "yarn build && concurrently \"tsc -w\" \"tsc-alias -w\"",
    "npm-pub": "yarn build && npm publish --access public",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "experiment": "yarn build && node dist/experiment.js",
    "new": "node ./dist/index.bin.js new"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "chalk-animation": "^2.0.3",
    "commander": "^11.0.0",
    "events": "^3.3.0",
    "figlet": "^1.6.0",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.2.8",
    "jscodeshift": "^17.1.2",
    "nanospinner": "^1.1.0",
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@jest/globals": "^29.6.2",
    "@types/chalk-animation": "^1.6.1",
    "@types/events": "^3.0.0",
    "@types/figlet": "^1.5.6",
    "@types/gradient-string": "^1.1.2",
    "@types/inquirer": "^9.0.3",
    "@types/jscodeshift": "^0.12.0",
    "@types/node": "^20.4.2",
    "@types/react": "^18.3.3",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "react": "^18.3.1",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  }
}
